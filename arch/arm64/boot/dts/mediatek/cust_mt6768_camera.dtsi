};

&kd_camera_hw1 {
	pinctrl-names = "default",
			"cam0_rst0", "cam0_rst1",
			"cam0_pnd0", "cam0_pnd1",
			"cam1_rst0", "cam1_rst1",
			"cam1_pnd0", "cam1_pnd1",
			"cam2_rst0", "cam2_rst1",
			"cam2_pnd0", "cam2_pnd1",

			"cam0_vcama_on",
			"cam0_vcama_off",
			"cam0_vcamd_on",
			"cam0_vcamd_off",
//			"cam1_vcama_on",
//			"cam1_vcama_off",
			"cam1_vcamd_on",
			"cam1_vcamd_off",
//			"cam2_vcama_on",
//			"cam2_vcama_off",
//			"cam2_vcamd_on",
//			"cam2_vcamd_off",

			"cam0_mclk_off",
			"cam0_mclk_2mA", "cam0_mclk_4mA",
			"cam0_mclk_6mA", "cam0_mclk_8mA",
			"cam1_mclk_off",
			"cam1_mclk_2mA", "cam1_mclk_4mA",
			"cam1_mclk_6mA", "cam1_mclk_8mA",
			"cam2_mclk_off",
			"cam2_mclk_2mA", "cam2_mclk_4mA",
			"cam2_mclk_6mA", "cam2_mclk_8mA";


	pinctrl-0 = <&camera_pins_default>;
	pinctrl-1 = <&camera0_rst_low>;
	pinctrl-2 = <&camera0_rst_high>;
	pinctrl-3 = <&camera0_pdn_low>;
	pinctrl-4 = <&camera0_pdn_high>;
	pinctrl-5 = <&camera1_rst_low>;
	pinctrl-6 = <&camera1_rst_high>;
	pinctrl-7 = <&camera1_pdn_low>;
	pinctrl-8 = <&camera1_pdn_high>;
	pinctrl-9 = <&camera2_rst_low>;
	pinctrl-10 = <&camera2_rst_high>;
	pinctrl-11 = <&camera2_pdn_low>;
	pinctrl-12 = <&camera2_pdn_high>;

	pinctrl-13 = <&camera0_vcama_on>;
	pinctrl-14 = <&camera0_vcama_off>;
	pinctrl-15 = <&camera0_vcamd_on>;
	pinctrl-16 = <&camera0_vcamd_off>;
//	pinctrl-17 = <&camera1_vcama_on>;
//	pinctrl-18 = <&camera1_vcama_off>;
	pinctrl-17 = <&camera1_vcamd_on>;
	pinctrl-18 = <&camera1_vcamd_off>;
//	pinctrl-21 = <&camera2_vcama_on>;
//	pinctrl-22 = <&camera2_vcama_off>;
//	pinctrl-23 = <&camera2_vcamd_on>;
//	pinctrl-24 = <&camera2_vcamd_off>;

	pinctrl-19 = <&camera0_mclk_off>;
	pinctrl-20 = <&camera0_mclk_2mA>;
	pinctrl-21 = <&camera0_mclk_4mA>;
	pinctrl-22 = <&camera0_mclk_6mA>;
	pinctrl-23 = <&camera0_mclk_8mA>;
	pinctrl-24 = <&camera1_mclk_off>;
	pinctrl-25 = <&camera1_mclk_2mA>;
	pinctrl-26 = <&camera1_mclk_4mA>;
	pinctrl-27 = <&camera1_mclk_6mA>;
	pinctrl-28 = <&camera1_mclk_8mA>;
	pinctrl-29 = <&camera2_mclk_off>;
	pinctrl-30 = <&camera2_mclk_2mA>;
	pinctrl-31 = <&camera2_mclk_4mA>;
	pinctrl-32 = <&camera2_mclk_6mA>;
	pinctrl-33 = <&camera2_mclk_8mA>;


	
	cam1_vcama-supply = <&mt_pmic_vcama2_ldo_reg>;
	

	
	cam2_vcama-supply = <&mt_pmic_vcama1_ldo_reg>;
	cam2_vcamd-supply = <&mt_pmic_vcamd_ldo_reg>;
	

	
	cam0_vcamio-supply = <&mt_pmic_vcamio_ldo_reg>;
	cam1_vcamio-supply = <&mt_pmic_vcamio_ldo_reg>;
	cam2_vcamio-supply = <&mt_pmic_vcamio_ldo_reg>;

	
	cam0_vcamaf-supply = <&mt_pmic_vldo28_ldo_reg>;

	cam0_enable_sensor = "ov64b40_mipi_raw s5kgm1st_mipi_raw ";
	cam1_enable_sensor = "ov16a1q_mipi_raw gc08a3sub_mipi_raw";
	cam2_enable_sensor = "s5k4h7yx_mipi_raw bf2253Lmacro_mipi_raw";
	
	status = "okay";

};

&camera_af_hw_node {
	camaf_m1_pmic-supply = <&mt_pmic_vldo28_ldo_reg>;
	camaf_m2_pmic-supply = <&mt_pmic_vldo28_ldo_reg>;
	camaf_m3_pmic-supply = <&mt_pmic_vldo28_ldo_reg>;
	status = "okay";
};


