/*prize-sunshuai-20200901, modify for charge start*/
&lk_charger {
    enable_pe_plus;
};

&charger {
	algorithm_name = "DualSwitchCharging";
	enable_pe_plus;
	enable_pe_2;
	enable_pe_3;
	parallel_vbus;
	battery_cv = <4400000>;
	min_charger_voltage = <4400000>;

	ac_charger_current = <2200000>;
	ac_charger_input_current = <3200000>;

	/* dual charger */
	chg1_ta_ac_charger_current = <1600000>;
	chg2_ta_ac_charger_current = <1700000>;

	min_charge_temp = <0>;
	min_charge_temp_plus_x_degree = <5>;
	max_charge_temp = <50>;
	max_charge_temp_minus_x_degree = <48>;

#if defined(CONFIG_PRIZE_CHARGE_CURRENT_CTRL_GIGAST)
	  start_step1_temp = <(0)>;
	  step1_max_current = <1320000>;
	  start_step2_temp = <15>;
	  start_step3_temp = <45>;
	  step2_max_current = <4400000>;
	  start_step4_temp = <50>;
	  step3_vot1_current = <1320000>;
	  step3_vot2_current = <840000>;
      temp_stp3_cv_voltage = <4100000>;
#endif
};
/*prize-sunshuai-20200901, modify for charge end*/




//prize add by lipengpeng wireless  start
&pio {
        charger_otg_pins_default: charger_otg_default {
        };
        charger_otg_off:  charger_otg_off {
            pins_cmd_dat {
                        pinmux = <PINMUX_GPIO52__FUNC_GPIO52>;
                        slew-rate = <1>;
                        output-low;
                };
        };
        charger_otg_on:  charger_otg_on {
            pins_cmd_dat {
                        pinmux = <PINMUX_GPIO52__FUNC_GPIO52>;
                        slew-rate = <1>;
                        output-high;
                };
        };
      wireless_5725_off:  wireless_5725_off {
            pins_cmd_dat {
                        pinmux = <PINMUX_GPIO160__FUNC_GPIO160>;
                        slew-rate = <1>;
                        output-high;
                };
        };
        wireless_5725_on:  wireless_5725_on {
            pins_cmd_dat {
                        pinmux = <PINMUX_GPIO160__FUNC_GPIO160>;
                        slew-rate = <1>;
                        output-low;
                };
        };
};
&i2c3 {
	wireless_chg_rx@2b {
		compatible = "maxictech,mt5725-15w";
		reg = <0x2b>;
		irq-gpio = <&pio 2 0>;//W_CHARGE_INT
		//trxset_gpio = <&pio 88 0>;//DC_EN
		//otgen_gpio = <&pio 87 0>;
		statu_gpio = <&pio 0 0>;//WRX_EN
		chipen_gpio = <&pio 160 0>;//CHARGE_EN
		//rx_power_capability = <10>;
		//one_pin_ctl = <1>;
		rx_power_capability = <13>;
	   one_pin_ctl = <0>;
	   pinctrl-names = "default", "charger_otg_off", "charger_otg_on",
				 "wireless_5725_off", "wireless_5725_on";
	   pinctrl-0 = <&charger_otg_pins_default>;
	   pinctrl-1 = <&charger_otg_off>;//WPC_OTG_EN
	   pinctrl-2 = <&charger_otg_on>;//WPC_OTG_EN
	   pinctrl-3 = <&wireless_5725_off>;//WPC_OTG_EN2
	   pinctrl-4 = <&wireless_5725_on>;//WPC_OTG_EN2
	};
};
